/* Toolbar size */
.TMP-thm-sz-toolbar {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding: 0 16px;
    height: 56px;
}

/* Elevations. See https://material.io/design/environment/elevation.html#default-elevations */
.TMP-thm-ele-none {
    z-index: 0;
    box-shadow: none
}

/* Card (resting), Raised button (resting), Quick entry / Search bar (resting) */
.TMP-thm-ele-button, .TMP-thm-ele-card {
    z-index: 2;
    box-shadow: 0 2px 4px rgba(0, 0, 0, .4)
}

.TMP-thm-ele-button:active, .TMP-thm-ele-button-raised, .TMP-thm-ele-card-raised {
    z-index: 8;
    box-shadow: 0 4px 8px rgba(0, 0, 0, .4)
}

/* App bar */
.TMP-thm-ele-bar {
    z-index: 4;
    box-shadow: 0 2px 4px rgba(0, 0, 0, .4)
}

/* Floating action button (FAB - resting) */
.TMP-thm-ele-floating {
    z-index: 6;
    box-shadow: 0 3px 6px rgba(0, 0, 0, .4)
}

.TMP-thm-ele-floating:active {
    z-index: 12;
    box-shadow: 0 6px 12px rgba(0, 0, 0, .4)
}

/* Bottom navigation bar, Menu, Card (when picked up), Raised button (pressed state) */
/* Sub menus */
.TMP-thm-ele-submenu-0 {
    z-index: 9;
    box-shadow: 0 4px 9px rgba(0, 0, 0, .4)
}

.TMP-thm-ele-submenu-1 {
    z-index: 10;
    box-shadow: 0 5px 10px rgba(0, 0, 0, .4)
}

.TMP-thm-ele-submenu-2 {
    z-index: 11;
    box-shadow: 0 5px 11px rgba(0, 0, 0, .4)
}

/* Nav drawer, Right drawer, Modal bottom sheet */
.TMP-thm-ele-nav {
    z-index: 16;
    box-shadow: 0 8px 16px rgba(0, 0, 0, .4)
}

/* Dialog, Picker */
.TMP-thm-ele-dialog {
    z-index: 24;
    box-shadow: 0 12px 24px rgba(0, 0, 0, .4)
}

